<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>AddHouseServlet</servlet-name>
        <servlet-class>servlet.AddHouseServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddHouseServlet</servlet-name>
        <url-pattern>/AddHouseServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>QueryAllHousesServlet</servlet-name>
        <servlet-class>servlet.QueryAllHousesServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>QueryAllHousesServlet</servlet-name>
        <url-pattern>/QueryAllHousesServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UpdateHouseServlet</servlet-name>
        <servlet-class>servlet.UpdateHouseServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UpdateHouseServlet</servlet-name>
        <url-pattern>/UpdateHouseServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>QueryByPageServlet</servlet-name>
        <servlet-class>servlet.QueryByPageServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>QueryByPageServlet</servlet-name>
        <url-pattern>/QueryByPageServlet</url-pattern>
    </servlet-mapping>

</web-app>